{
  // "javascript.validate.enable": false,
  "todo.embedded.exclude": [
    "**/.!(todo|todos|task|tasks)/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**"
  ],
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  /* "javascript.suggest.enabled": false,
  "typescript.suggest.enabled": false,
  "typescript.validate.enable": false */
  /* "files.associations": {
    "webpack.*.config.js": "webpack"
  }, */
  "material-icon-theme.files.associations": {
    "webpack.assets.config.js": "webpack"
  },
  "commands.commands": [
    {
      "text": "$(sync) Refresh langs",
      "command": "workbench.action.terminal.sendSequence",
      "arguments": [{ "text": "npm run i18n\n" }],
      "color": "#bdc3c7",
      "filterFileRegex": "BrainlyTools_Extension"
    },
    {
      "text": "$(sync) Refresh English",
      "command": "workbench.action.terminal.sendSequence",
      "arguments": [{ "text": "npm run i18n en_US\n" }],
      "color": "#ecf0f1",
      "filterFileRegex": "BrainlyTools_Extension"
    },
    {
      "text": "$(file-code) Open en_US.json",
      "command": "workbench.action.tasks.runTask",
      "arguments": ["openSourceLangFile"],
      "color": "#ecf0f1",
      "filterFileRegex": "BrainlyTools_Extension"
    },
    {
      "text": "$(terminal)",
      "command": "workbench.action.terminal.focus",
      "tooltip": "Focus to terminal #1",
      "alignment": "right"
    }
  ]
}
